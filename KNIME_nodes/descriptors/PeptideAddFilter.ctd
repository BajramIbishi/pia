<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<tool category="peptide" name="PeptideAddFilter" version="1.0.0-rc2">
    <description>Adds a peptide level filter to a specified file. The file is given by the first parameter, which has to be the file ID. Filters are added by their name, an abbreviation for the camparison, the compared value and (optional), whether the comparison should be negated e.g. &quot;AddFilter=1,charge_filter,EQ,2,no&quot;. 
Registered peptide filters are: [peptide_spectrum_title_list_filter, peptide_unique_filter, peptide_sequence_filter, psm_score_filter_[scoreShort], peptide_modifications_filter, nr_psms_per_peptide_filter, peptide_missed_cleavages_filter, peptide_description_filter, peptide_source_id_list_filter, nr_spectra_per_peptide_filter, peptide_rank_filter, peptide_accessions_filter, peptide_file_list_filter, peptide_score_filter_[scoreShort]]</description>
    <executableName>java</executableName>
    <cli>
        <clielement required="false" isList="false" optionIdentifier="">
            <mapping referenceName="PeptideAddFilter.vmsettings"/>
        </clielement>
        <clielement required="true" isList="false" optionIdentifier="-cp">
            <mapping referenceName="PeptideAddFilter.classpath"/>
        </clielement>
        <clielement required="true" isList="false" optionIdentifier="de.mpc.pia.modeller.PIAModeller"/>
        <clielement required="true" isList="false" optionIdentifier="-paramFile">
            <mapping referenceName="PeptideAddFilter.paramFile"/>
        </clielement>
        <clielement required="true" isList="false" optionIdentifier="-paramOutFile">
            <mapping referenceName="PeptideAddFilter.paramOutFile"/>
        </clielement>
        <clielement required="true" isList="false" optionIdentifier="-append"/>
        <clielement required="true" isList="false" optionIdentifier="PeptideAddFilter"/>
        <clielement required="false" isList="false" optionIdentifier="">
            <mapping referenceName="PeptideAddFilter.file ID"/>
        </clielement>
        <clielement required="false" isList="false" optionIdentifier="">
            <mapping referenceName="PeptideAddFilter.filtername"/>
        </clielement>
        <clielement required="false" isList="false" optionIdentifier="">
            <mapping referenceName="PeptideAddFilter.comparison"/>
        </clielement>
        <clielement required="false" isList="false" optionIdentifier="">
            <mapping referenceName="PeptideAddFilter.value"/>
        </clielement>
        <clielement required="false" isList="false" optionIdentifier="">
            <mapping referenceName="PeptideAddFilter.negate"/>
        </clielement>
    </cli>
    <PARAMETERS version="1.6.2">
        <NODE description="Adds a peptide level filter to a specified file. The file is given by the first parameter, which has to be the file ID. Filters are added by their name, an abbreviation for the camparison, the compared value and (optional), whether the comparison should be negated e.g. &quot;AddFilter=1,charge_filter,EQ,2,no&quot;. &#xA;Registered peptide filters are: [peptide_spectrum_title_list_filter, peptide_unique_filter, peptide_sequence_filter, psm_score_filter_[scoreShort], peptide_modifications_filter, nr_psms_per_peptide_filter, peptide_missed_cleavages_filter, peptide_description_filter, peptide_source_id_list_filter, nr_spectra_per_peptide_filter, peptide_rank_filter, peptide_accessions_filter, peptide_file_list_filter, peptide_score_filter_[scoreShort]]" name="PeptideAddFilter">
            <ITEM value="1.0.0-rc2" advanced="true" required="true" description="Version of the tool that generated this parameters file." type="string" name="version"/>
            <ITEM value="-Xmx1G" advanced="true" required="false" description="Additional Java settings, like giving Java a big enough amount of memory." type="string" name="vmsettings"/>
            <ITEM value="pia.jar" advanced="true" required="false" description="The actual JAR, in which the PIA classes reside." type="string" name="classpath"/>
            <ITEM value="" advanced="false" required="true" supported_formats="*.xml" description="Path to the file, which contains the build pipeline until this step." type="input-file" name="paramFile"/>
            <ITEM value="" advanced="false" required="true" supported_formats="*.xml" description="Path to the file, which will contain the pipeline with added filters." type="output-file" name="paramOutFile"/>
            <ITEM value="" advanced="false" required="true" description="" type="string" name="file ID"/>
            <ITEM value="" advanced="false" required="true" description="" type="string" name="filtername"/>
            <ITEM value="" advanced="false" required="true" restrictions="LT,LEQ,EQ,GEQ,GT,CON,COO,REG,RXO,HAM,MAS,DEC,RES" description="" type="string" name="comparison"/>
            <ITEM value="" advanced="false" required="true" description="" type="string" name="value"/>
            <ITEM value="" advanced="false" required="true" restrictions="no,yes" description="" type="string" name="negate"/>
        </NODE>
    </PARAMETERS>
</tool>
